{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir":"build",
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "allowJs": false,
    "strict": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "src",
    ".eslintrc.js",
    "babel.config.json",
    "./src/**/*.test.ts",
    "./main.ts",
    "jest.config.ts"
  ],
  "exclude": [
    "node_modules"],
}

